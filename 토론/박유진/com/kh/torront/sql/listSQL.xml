<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>
<sqlMap  namespace="list">

	<typeAlias alias="list2" type="com.kh.torront.vo.ListVO" />
	
	<!-- 리절트 맵 정의 -->
	  	<resultMap id="list" class="com.kh.torront.vo.ListVO">
	  		<result property="id" column="id"/>
		  	<result property="subject" column="subject"/>
		  	<result property="contents" column="contents"/>
		</resultMap>
		<resultMap class="com.kh.torront.vo.ListVO" id="list2">
			<result property="id" column="id"/>
		  	<result property="subject" column="subject"/>
		  	<result property="contents" column="contents"/>
		</resultMap>
	  
		<!-- insert 쿼리문 정의 -->
		<insert id="insert" parameterClass="list2" >
		
      		INSERT INTO LIST (SUBJECT
                         )
                	 VALUES (#subject#
                         )
		</insert>
		
		<!-- select 쿼리문 정의 -->
		<select id="select" parameterClass="list" resultMap="String">
			SELECT contents FROM LIST 
		</select>
	
</sqlMap>

